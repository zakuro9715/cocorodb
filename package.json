  "devDependencies": {
    "@kazupon/vue-i18n-loader": "^0.3.0",
    "@storybook/vue": "^3.4.3",
    "babel-eslint": "^8.2.3",
    "eslint": "^4.19.1",
    "eslint-config-zakuro": "^3.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-vue": "^4.5.0",
    "husky": "^0.14.3",
